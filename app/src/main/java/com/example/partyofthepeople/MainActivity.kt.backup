class MainActivity : ComponentActivity() {
    private val googleSignInClient by lazy {
        val googleSignInOptions = GoogleSignInOptions.Builder(GoogleSignInOptions.DEFAULT_SIGN_IN)
            .requestIdToken(getString(R.string.default_web_client_id))
            .requestEmail()
            .build()
        GoogleSignIn.getClient(this, googleSignInOptions)
    }

    private val authViewModel: AuthViewModel by viewModels()
    
    // Define the ActivityResultLauncher for Google Sign-In
    private val googleSignInLauncher = registerForActivityResult(
        ActivityResultContracts.StartActivityForResult()
    ) { result ->
        if (result.resultCode == RESULT_OK) {
            val data = result.data
            Log.d("MainActivity", "Received Google Sign In result")
            val task = GoogleSignIn.getSignedInAccountFromIntent(data)
            try {
                val account = task.getResult(ApiException::class.java)
                Log.d("MainActivity", "Google sign in succeeded: ${account.email}")
                val credential = GoogleAuthProvider.getCredential(account.idToken, null)
                authViewModel.signInWithGoogle(credential)
            } catch (e: ApiException) {
                Log.e("MainActivity", "Google sign in failed", e)
            }
        }
    }

    override fun onCreate(savedInstanceState: Bundle?) {
        super.onCreate(savedInstanceState)

        Log.d("MainActivity", "onCreate called")

        setContent {
            MaterialTheme {
                var currentScreen by remember { mutableStateOf("splash") }
                val userProfile by authViewModel.userProfile.collectAsStateWithLifecycle(initialValue = null)
                val mainViewModel = viewModel<MainViewModel>()

                // Check for achievements when user profile changes
                LaunchedEffect(userProfile) {
                    userProfile?.let { profile ->
                        Log.d("MainActivity", "UserProfile state changed: ${profile.username}")
                        
                        // Check if there's a latest achievement to show
                        profile.latestAchievement?.let { achievement ->
                            // Only show if earned in the last 60 seconds
                            val currentTime = System.currentTimeMillis()
                            val achievementTime = achievement.dateEarned
                            if (currentTime - achievementTime < 60000) {
                                mainViewModel.addPendingAchievement(achievement)
                            }
                        }
                    }
                }

                LaunchedEffect(currentScreen) {
                    Log.d("MainActivity", "Current screen changed to: $currentScreen")
                }

                // Initial splash screen delay
                LaunchedEffect(Unit) {
                    try {
                        Log.d("MainActivity", "Starting splash screen delay")
                        delay(3000)
                        currentScreen = if (userProfile == null) {
                            Log.d("MainActivity", "No user profile, showing login")
                            "login"
                        } else {
                            Log.d("MainActivity", "User profile exists, showing main")
                            "main"
                        }
                    } catch (e: Exception) {
                        Log.e("MainActivity", "Error in splash screen delay", e)
                        currentScreen = "login"
                    }
                }

                Surface(
                    modifier = Modifier.fillMaxSize(),
                    color = Color(0xFF1D3557)
                ) {
                    when (currentScreen) {
                        "splash" -> SplashScreen()
                        "login" -> LoginScreen(
                            onGuestLogin = {
                                Log.d("MainActivity", "Guest login clicked")
                                authViewModel.signInAnonymously()
                            },
                            onGoogleLogin = {
                                Log.d("MainActivity", "Google login clicked")
                                startGoogleSignIn()
                            }
                        )
                        "main" -> MainContent(
                            userProfile = userProfile,
                            onLogout = {
                                Log.d("MainActivity", "Logout clicked")
                                Firebase.auth.signOut()
                                googleSignInClient.signOut()
                                currentScreen = "login"
                            }
                        )
                    }
                }

                // Observer for authentication state
                LaunchedEffect(userProfile) {
                    if (userProfile != null && currentScreen != "main") {
                        Log.d("MainActivity", "Authenticated, switching to main screen")
                        currentScreen = "main"
                    }
                }
            }
        }
    }

    private fun startGoogleSignIn() {
        Log.d("MainActivity", "Starting Google Sign In flow")
        val signInIntent = googleSignInClient.signInIntent
        googleSignInLauncher.launch(signInIntent)
    }
}

class MainViewModel : ViewModel() {
    private val _currentScreen = mutableStateOf("main")
    val currentScreen: State<String> = _currentScreen

    private val _currentProposal = mutableStateOf<String?>(null)
    val currentProposal: State<String?> = _currentProposal
    
    // Achievement notification system
    private val _pendingAchievements = mutableStateListOf<Achievement>()
    val pendingAchievements: List<Achievement> = _pendingAchievements
    
    fun navigateToScreen(screen: String) {
        _currentScreen.value = screen
    }

    fun navigateToVoteScreen(proposal: String) {
        _currentScreen.value = "vote_detail"
        _currentProposal.value = proposal
    }
    
    fun navigateToAchievementsScreen() {
        _currentScreen.value = "achievements"
    }
    
    fun navigateToLeaderboardScreen() {
        _currentScreen.value = "leaderboard"
    }

    fun navigateToBipartisanMatchScreen() {
        _currentScreen.value = "bipartisan_match"
    }

    fun navigateToElectionHistoryScreen() {
        _currentScreen.value = "election_history"
    }

    fun navigateToControversyRadarScreen() {
        _currentScreen.value = "controversy_radar"
    }

    fun navigateToProfileCustomizationScreen() {
        _currentScreen.value = "profile_customization"
    }
    
    fun navigateToTitleScreen() {
        _currentScreen.value = "title_screen"
    }

    fun navigateBack() {
        _currentScreen.value = "main"
    }
    
    fun addPendingAchievement(achievement: Achievement) {
        _pendingAchievements.add(achievement)
    }
    
    fun removePendingAchievement(achievement: Achievement) {
        _pendingAchievements.remove(achievement)
    }
}

@Composable
fun MainContent(
    userProfile: UserProfile?,
    onLogout: () -> Unit,
    viewModel: MainViewModel = viewModel()
) {
    val currentScreen by viewModel.currentScreen
    val currentProposal by viewModel.currentProposal
    var freedomBucks by remember { mutableIntStateOf(userProfile?.freedomBucks ?: 1776) }
    val pendingAchievements = viewModel.pendingAchievements

    Box(modifier = Modifier.fillMaxSize()) {
        Column {
            TopBar(freedomBucks = freedomBucks)
            ProgressTracker()

            when (currentScreen) {
                "main" -> MainScreen(
                    onBattlegroundClick = { viewModel.navigateToScreen("battleground") },
                    onDebateClick = { viewModel.navigateToScreen("debate") },
                    onMissionClick = { viewModel.navigateToScreen("missions") },
                    onProfileClick = { viewModel.navigateToScreen("profile") },
                    onLeaderboardClick = { viewModel.navigateToLeaderboardScreen() }
                )
                "debate" -> DebateScreen(
                    onBackClick = { viewModel.navigateBack() }
                )
                "battleground" -> BattlegroundScreen(
                    onBackClick = { viewModel.navigateBack() }
                )
                "vote_detail" -> currentProposal?.let { proposal ->
                    VoteDetailScreen(proposal = proposal, viewModel = viewModel)
                }
                "profile" -> {
                    userProfile?.let { profile ->
                        ProfileScreen2(
                            profile = profile,
                            onBackClick = { viewModel.navigateBack() },
                            onViewAllAchievements = { viewModel.navigateToAchievementsScreen() },
                            onViewBipartisanMatch = { viewModel.navigateToBipartisanMatchScreen() },
                            onViewElectionHistory = { viewModel.navigateToElectionHistoryScreen() },
                            onViewControversyRadar = { viewModel.navigateToControversyRadarScreen() },
                            onCustomizeProfile = { viewModel.navigateToProfileCustomizationScreen() },
                            onManageTitles = { viewModel.navigateToTitleScreen() }
                        )
                    }
                }
                "achievements" -> {
                    userProfile?.let { profile ->
                        AchievementsScreen(
                            achievements = profile.achievements,
                            onBackClick = { viewModel.navigateToScreen("profile") }
                        )
                    }
                }
                "leaderboard" -> {
                    LeaderboardScreen(
                        onBackClick = { viewModel.navigateBack() }
                    )
                }
                "bipartisan_match" -> {
                    userProfile?.let { profile ->
                        BipartisanMatchScreen(
                            userProfile = profile,
                            onBackClick = { viewModel.navigateToScreen("profile") }
                        )
                    }
                }
                "election_history" -> {
                    userProfile?.let { profile ->
                        ElectionHistoryScreen(
                            userProfile = profile,
                            onBackClick = { viewModel.navigateToScreen("profile") }
                        )
                    }
                }
                "controversy_radar" -> {
                    userProfile?.let { profile ->
                        ControversyRadarScreen(
                            userProfile = profile,
                            onBackClick = { viewModel.navigateToScreen("profile") }
                        )
                    }
                }
                "profile_customization" -> {
                    userProfile?.let { profile ->
                        ProfileCustomizationScreen(
                            userProfile = profile,
                            onBackClick = { viewModel.navigateToScreen("profile") }
                        )
                    }
                }
                "title_screen" -> {
                    userProfile?.let { profile ->
                        val customNavController = androidx.navigation.compose.rememberNavController()
                        
                        // Create a custom back handler
                        val onBackClick = {
                            viewModel.navigateToScreen("profile")
                        }
                        
                        // BackHandler to override the default behavior
                        BackHandler {
                            onBackClick()
                        }
                        
                        com.example.partyofthepeople.profile.TitleScreen(
                            navController = customNavController,
                            userProfile = profile,
                            onBackClick = onBackClick
                        )
                    }
                }
                else -> Text("Coming Soon!", color = Color.White)
            }
        }
        
        // Show achievement notifications (if any)
        if (pendingAchievements.isNotEmpty()) {
            AchievementNotificationHost(
                achievements = pendingAchievements,
                onAchievementDismissed = { achievement ->
                    viewModel.removePendingAchievement(achievement)
                }
            )
        }
    }
}

@Composable
fun VoteDetailScreen(proposal: String, viewModel: MainViewModel = viewModel()) {
    var hasVoted by remember { mutableStateOf(false) }
    var userVote by remember { mutableStateOf<Boolean?>(null) }
    var selectedTab by remember { mutableStateOf(0) }
    val tabs = listOf("Discussion", "Details", "Stats")
    
    // Mock data
    val yesVotes = remember { (100..5000).random() }
    val noVotes = remember { (100..5000).random() }
    val totalVotes = yesVotes + noVotes
    val yesPercentage = (yesVotes.toFloat() / totalVotes * 100).toInt()
    
    // Focus on comment section after clicking Sound Off button
    var focusOnComments by remember { mutableStateOf(false) }
    
    // Create a stable postId for this proposal
    val postId = remember { proposal.hashCode().toString() }
    
    // After voting, automatically trigger sample comments for discussion
    LaunchedEffect(hasVoted) {
        if (hasVoted) {
            // Small delay before showing comments to simulate server interaction
            delay(500)
            focusOnComments = true
        }
    }
    
    LaunchedEffect(focusOnComments) {
        if (focusOnComments) {
            selectedTab = 0 // Switch to Discussion tab
        }
    }
    
    Column(
        modifier = Modifier
            .fillMaxSize()
            .background(Color(0xFF1D3557))
            .padding(16.dp)
    ) {
        // Header with back button
        Row(
            modifier = Modifier.fillMaxWidth(),
            verticalAlignment = Alignment.CenterVertically
        ) {
            IconButton(
                onClick = { viewModel.navigateBack() }
            ) {
                Icon(
                    imageVector = Icons.AutoMirrored.Filled.ArrowBack,
                    contentDescription = "Back",
                    tint = Color.White
                )
            }
            
            Text(
                text = "Law Proposal",
                fontSize = 20.sp,
                fontWeight = FontWeight.Bold,
                color = Color.White
            )
        }
        
        Spacer(modifier = Modifier.height(16.dp))
        
        // Proposal Card
        Card(
            colors = CardDefaults.cardColors(containerColor = Color(0xFF2A4A73)),
            modifier = Modifier.fillMaxWidth()
        ) {
            Column(
                modifier = Modifier.padding(16.dp)
            ) {
                Text(
                    text = proposal,
                    color = Color.White,
                    fontSize = 22.sp,
                    fontWeight = FontWeight.Bold
                )
                
                Spacer(modifier = Modifier.height(16.dp))
                
                // Vote Progress Bar
                Column {
                    Row(
                        modifier = Modifier.fillMaxWidth(),
                        horizontalArrangement = Arrangement.SpaceBetween
                    ) {
                        Text("Yes: $yesPercentage%", color = Color(0xFF4CAF50))
                        Text("No: ${100 - yesPercentage}%", color = Color(0xFFE63946))
                    }
                    
                    Spacer(modifier = Modifier.height(4.dp))
                    
                    Row(
                        modifier = Modifier
                            .fillMaxWidth()
                            .height(12.dp)
                            .clip(RoundedCornerShape(6.dp))
                    ) {
                        Box(
                            modifier = Modifier
                                .weight(yesPercentage.toFloat())
                                .height(12.dp)
                                .background(Color(0xFF4CAF50))
                        )
                        Box(
                            modifier = Modifier
                                .weight((100 - yesPercentage).toFloat())
                                .height(12.dp)
                                .background(Color(0xFFE63946))
                        )
                    }
                    
                    Spacer(modifier = Modifier.height(4.dp))
                    
                    Text(
                        text = "Total votes: $totalVotes",
                        color = Color.LightGray,
                        fontSize = 12.sp
                    )
                }
                
                Spacer(modifier = Modifier.height(24.dp))
                
                // Voting Buttons
                if (!hasVoted) {
                    Row(
                        modifier = Modifier.fillMaxWidth(),
                        horizontalArrangement = Arrangement.SpaceEvenly
                    ) {
                        Button(
                            onClick = { 
                                hasVoted = true
                                userVote = true
                                // Record vote logic would go here
                            },
                            colors = ButtonDefaults.buttonColors(containerColor = Color(0xFF4CAF50)),
                            modifier = Modifier.weight(1f)
                        ) {
                            Text("SUPPORT ✅")
                        }
                        
                        Spacer(modifier = Modifier.width(16.dp))
                        
                        Button(
                            onClick = { 
                                hasVoted = true
                                userVote = false
                                // Record vote logic would go here
                            },
                            colors = ButtonDefaults.buttonColors(containerColor = Color(0xFFE63946)),
                            modifier = Modifier.weight(1f)
                        ) {
                            Text("OPPOSE ❌")
                        }
                    }
                } else {
                    Box(
                        modifier = Modifier
                            .fillMaxWidth()
                            .padding(16.dp),
                        contentAlignment = Alignment.Center
                    ) {
                        Card(
                            colors = CardDefaults.cardColors(
                                containerColor = if (userVote == true) Color(0xFF4CAF50).copy(alpha = 0.2f)
                                                else Color(0xFFE63946).copy(alpha = 0.2f)
                            )
                        ) {
                            Text(
                                text = if (userVote == true) "You supported this proposal ✅" 
                                        else "You opposed this proposal ❌",
                                modifier = Modifier.padding(16.dp),
                                color = if (userVote == true) Color(0xFF4CAF50) else Color(0xFFE63946),
                                fontWeight = FontWeight.Bold
                            )
                        }
                        
                        // After voting, show the SoundOffButton to encourage comments
                        com.example.partyofthepeople.ui.components.SoundOffButton(
                            onClick = { focusOnComments = true },
                            isAfterVote = true
                        )
                    }
                }
            }
        }
        
        Spacer(modifier = Modifier.height(16.dp))
        
        // Tabs
        TabRow(selectedTabIndex = selectedTab) {
            tabs.forEachIndexed { index, title ->
                Tab(
                    selected = selectedTab == index,
                    onClick = { selectedTab = index },
                    text = { Text(title) }
                )
            }
        }
        
        when (selectedTab) {
            0 -> {
                // Discussion tab
                Text(
                    text = "Discussion",
                    color = Color(0xFFFFD700),
                    fontSize = 18.sp,
                    fontWeight = FontWeight.Bold,
                    modifier = Modifier.padding(vertical = 8.dp)
                )
                
                Box(
                    modifier = Modifier
                        .fillMaxWidth()
                        .weight(1f)
                        .background(Color(0xFF0A1929).copy(alpha = 0.5f))
                        .clip(RoundedCornerShape(8.dp))
                ) {
                    // Use CommentSection with the consistent postId
                    CommentsSection(postId = postId)
                }
            }
            1 -> {
                // Details tab
                Text(
                    text = "Proposal Details",
                    color = Color(0xFFFFD700),
                    fontSize = 18.sp,
                    fontWeight = FontWeight.Bold,
                    modifier = Modifier.padding(vertical = 8.dp)
                )
                
                Card(
                    colors = CardDefaults.cardColors(containerColor = Color(0xFF0A1929).copy(alpha = 0.5f)),
                    modifier = Modifier.weight(1f)
                ) {
                    Column(modifier = Modifier.padding(16.dp)) {
                        DetailRow("Sponsor", "Patriot #12345")
                        DetailRow("Submitted", "3 days ago")
                        DetailRow("Category", "General")
                        DetailRow("Status", "Open for voting")
                        DetailRow("Closes", "In 4 days")
                        
                        Spacer(modifier = Modifier.height(16.dp))
                        
                        Text(
                            text = "Full Description:",
                            color = Color.White,
                            fontWeight = FontWeight.Bold
                        )
                        
                        Spacer(modifier = Modifier.height(8.dp))
                        
                        Text(
                            text = "This is a detailed explanation of the proposal that outlines the reasoning behind it and potential impacts of implementation. Citizens should consider all aspects before casting their vote.",
                            color = Color.White
                        )
                    }
                }
            }
            2 -> {
                // Stats tab
                Text(
                    text = "Voting Statistics",
                    color = Color(0xFFFFD700),
                    fontSize = 18.sp,
                    fontWeight = FontWeight.Bold,
                    modifier = Modifier.padding(vertical = 8.dp)
                )
                
                Card(
                    colors = CardDefaults.cardColors(containerColor = Color(0xFF0A1929).copy(alpha = 0.5f)),
                    modifier = Modifier.weight(1f)
                ) {
                    Column(modifier = Modifier.padding(16.dp)) {
                        DetailRow("Yes Votes", yesVotes.toString())
                        DetailRow("No Votes", noVotes.toString())
                        DetailRow("Total Participation", "$totalVotes citizens")
                        DetailRow("Engagement Rate", "23%")
                        
                        Spacer(modifier = Modifier.height(16.dp))
                        
                        Text(
                            text = "Voting Breakdown by State:",
                            color = Color.White,
                            fontWeight = FontWeight.Bold
                        )
                        
                        Spacer(modifier = Modifier.height(8.dp))
                        
                        DetailRow("New York", "${(20..80).random()}% support")
                        DetailRow("Texas", "${(20..80).random()}% support")
                        DetailRow("California", "${(20..80).random()}% support")
                        DetailRow("Florida", "${(20..80).random()}% support")
                    }
                }
            }
        }
    }
}

